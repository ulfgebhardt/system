<div class="masthead">
    <h2 class="muted">Userinfo for User: ${username}</h2>
    <h4 class="text-info">Basics</h4>
</div>
<table class="table">
    <tr>
        <th>ID</th>
        <td>${id}</td>
    </tr>
    <tr>
        <th>Username</th>
        <td>${username}</td>
    </tr>
    <tr>
        <th>EMail</th>
        <td>${email}</td>
    </tr>
    <tr>
        <th>Joindate</th>
        <td>${joindate}</td>
    </tr>
    <tr>
        <th>Locale</th>
        <td>${locale}</td>
    </tr>
    <tr>
        <th>Last active</th>
        <td>${time_elapsed}</td>
    </tr>
    <tr>
        <th>Flag(remove)</th>
        <td>${account_flag}</td>
    </tr>
</table>
<h4 class="text-info">Users Last Actions</h4>
${user_actions}
